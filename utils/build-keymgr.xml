<project name="jfkeymgr" default="jar" basedir=".">
  <description>jfkeymgr</description>
  <!-- set global properties for this build -->
  <property name="app" value="jfkeymgr"/>
  <property name="apptype" value="console"/>
  <property name="cfg" value="cfg/jfkeymgr.cfg"/>
  <property name="ico" value="javaforce.ico"/>
  <property name="home" location=".."/>

  <property environment="env"/>

  <import file="${home}/base.xml"/>

  <target name="depjars" depends="depnatives">
  </target>

  <target name="compile" depends="depjars" description="compile the source">
  </target>

  <target name="jar" depends="depjars,compile" description="build jar file">
  </target>

  <target name="install" description="install files for linux">
  </target>
</project>
