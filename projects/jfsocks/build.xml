<project name="jfsocks-config" default="jar" basedir=".">
  <description>SOCKS Server Config Tool</description>
  <!-- set global properties for this build -->
  <property name="app" value="jfsocks"/>
  <property name="home" location="../.."/>
  <property name="version" value="0.11"/>
  <property name="jre" location="${home}/jre_base_desktop"/>

  <property name="project2" value="build-service"/>
  <property name="project3" value="build-client"/>

  <import file="../../base.xml"/>

  <target name="depjars" description="jar">
    <copy file="../../jars/javaforce.jar" todir="."/>
  </target>

  <target name="jar" depends="depjars" description="jar">
  </target>

  <!-- sudo ant install -->
  <target name="install" description="install files">
  </target>
</project>
